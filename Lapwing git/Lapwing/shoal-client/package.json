    "grunt-karma": "2.0.0",
    "grunt-ng-annotate": "3.0.0",
    "grunt-ng-constant": "2.0.1",
    "http-proxy": "1.13.1",
    "http-server": "0.8.5",
    "jasmine": "2.5.2",
    "jasmine-reporters": "2.1.1",
    "jslint": "0.9.6",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
